{
  "name": "Pretty Copy",
  "version" : "1.0",
  "description": "Pretty Copy current urls",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus",
    "clipboardWrite",
    "clipboardRead"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
